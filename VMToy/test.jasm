; test assembly outputting a number of dots
INIT 0 40	; other comment
MOV A 0	; put a 0 into register named A
:loop		; start of cycle

; writing tens
MOV B A
DIV B 10
ADD B '0'
DBG 0 B

; writing units
MOV B A
MOD B 10
ADD B '0'
DBG 0 B

; writing newline
DBG 0 13
DBG 0 10

; incrementing
ADD A 1		

CMP A 30     
JNA loop		; while A < 30

TRM			; terminate
